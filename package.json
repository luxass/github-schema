{
  "name": "github-schema",
  "version": "1.20.0",
  "description": "GitHub's GraphQL schema",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@10.13.1",
  "license": "MIT",
  "homepage": "https://github.com/luxass/github-schema",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luxass/github-schema.git"
  },
  "bugs": {
    "url": "https://github.com/luxass/github-schema/issues"
  },
  "keywords": [
    "github graphql",
    "github",
    "graphql",
    "github schema"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./github-schema": {
      "import": "./dist/github-schema.js",
      "require": "./dist/github-schema.cjs"
    },
    "./raw": {
      "import": "./dist/raw.js",
      "require": "./dist/raw.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "files": [
    "dist",
    "github-schema.d.ts",
    "github-schema.graphql"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsdown",
    "schema:download": "pnpx tsx --env-file=.env ./scripts/download.ts",
    "schema:codegen": "graphql-codegen",
    "prepublishOnly": "pnpm run schema:download && pnpm run schema:codegen && pnpm run build",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/typescript": "^4.1.6",
    "@luxass/eslint-config": "^5.1.0",
    "@types/node": "^20.14.10",
    "eslint": "^9.30.1",
    "eslint-plugin-format": "^1.0.1",
    "publint": "^0.3.12",
    "tsdown": "^0.12.9",
    "typescript": "^5.8.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
