{
  "name": "github-schema",
  "type": "module",
  "version": "1.1.5",
  "packageManager": "bun@1.0.13",
  "description": "GitHub's GraphQL schema",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "license": "MIT",
  "homepage": "https://github.com/luxass/github-schema",
  "repository": {
    "type": "git",
    "url": "https://github.com/luxass/github-schema"
  },
  "bugs": {
    "url": "https://github.com/luxass/github-schema/issues"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./raw": {
      "import": {
        "types": "./dist/raw.d.ts",
        "default": "./dist/raw.mjs"
      },
      "require": {
        "types": "./dist/raw.d.cts",
        "default": "./dist/raw.cjs"
      }
    },
    "./graphql-schema": "./github-schema.graphql",
    "./github-schema": {
      "import": {
        "types": "./dist/github-schema.d.ts",
        "default": "./dist/github-schema.mjs"
      },
      "require": {
        "types": "./dist/github-schema.d.cts",
        "default": "./dist/github-schema.cjs"
      }
    },
    "./schema": {
      "import": {
        "types": "./dist/github-schema.d.ts",
        "default": "./dist/github-schema.mjs"
      },
      "require": {
        "types": "./dist/github-schema.d.cts",
        "default": "./dist/github-schema.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "github-schema.d.ts",
    "github-schema.graphql"
  ],
  "scripts": {
    "build": "tsup",
    "schema:download": "bun run scripts/download.ts",
    "schema:codegen": "graphql-codegen",
    "pkg:bump": "bun run --silent scripts/bump.ts",
    "prepublishOnly": "bun run schema:download && bun run schema:codegen && bun run build",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@luxass/eslint-config": "^4.0.0-beta.4",
    "bun-types": "^1.0.13",
    "eslint": "^8.53.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
