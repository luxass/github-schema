{
  "name": "github-schema",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "bun@1.0.2",
  "description": "GitHub's GraphQL schema",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "license": "MIT",
  "homepage": "https://github.com/luxass/github-schema",
  "repository": {
    "type": "git",
    "url": "https://github.com/luxass/github-schema"
  },
  "bugs": {
    "url": "https://github.com/luxass/github-schema/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./github-schema.graphql": "./github-schema.graphql",
    "./github-schema": "./github-schema.d.ts",
    "./schema": "./github-schema.d.ts",
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "github-schema.graphql",
    "github-schema.d.ts"
  ],
  "scripts": {
    "build": "unbuild",
    "download": "bun run scripts/download.ts",
    "codegen": "graphql-codegen",
    "prepublishOnly": "bun run download && bun run codegen && bun run build",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@luxass/eslint-config": "^3.3.2",
    "bun-types": "^1.0.2",
    "eslint": "^8.49.0",
    "eslint-plugin-import": "npm:eslint-plugin-i@2.29.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0"
  }
}
