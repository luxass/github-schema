{
  "name": "github-schema",
  "version": "1.20.15",
  "description": "GitHub's GraphQL schema",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@10.17.1",
  "license": "MIT",
  "homepage": "https://github.com/luxass/github-schema",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luxass/github-schema.git"
  },
  "bugs": {
    "url": "https://github.com/luxass/github-schema/issues"
  },
  "keywords": [
    "github graphql",
    "github",
    "graphql",
    "github schema"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./github-schema": {
      "import": "./dist/github-schema.js",
      "require": "./dist/github-schema.cjs"
    },
    "./raw": {
      "import": "./dist/raw.js",
      "require": "./dist/raw.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "files": [
    "dist",
    "github-schema.d.ts",
    "github-schema.graphql"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsdown",
    "schema:download": "node --env-file=.env ./scripts/download.ts",
    "schema:codegen": "graphql-codegen",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "graphql": "16.11.0",
    "graphql-tag": "2.12.6"
  },
  "devDependencies": {
    "@graphql-codegen/add": "6.0.0",
    "@graphql-codegen/cli": "6.0.0",
    "@graphql-codegen/typescript": "5.0.0",
    "@luxass/eslint-config": "5.3.2",
    "@types/node": "20.19.17",
    "eslint": "^9.35.0",
    "eslint-plugin-format": "^1.0.1",
    "publint": "0.3.13",
    "tsdown": "0.15.4",
    "typescript": "5.9.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
